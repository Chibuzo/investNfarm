section.xs-header-top
  .container
    .row
      .col-lg-6.col-md-8
        .header-top-info
          ul
            li
              i.icon.icon-map-marker2
              |  16, Adeniyi Jones, Victoria Island, Lagos. Nigeria
            li
              i.icon.icon-envelope
              | investnfarm@gmail.com
      .col-lg-6.align-self-center.col-md-4
        .header-top-social
          ul
            //- li
            //-   a(href='#')
            //-     i.fa.fa-facebook-f
            li
              a(href='https://twitter.com/investnfarm')
                i.fa.fa-twitter
            li
              a(href='https://instagram.com/investnfarm/')
                i.fa.fa-instagram
section.xs-header-middle
  .container
    .row
      .col-md-3.align-self-center
        .logo
          a(href='/')
            img(src='/images/logo.png' alt='')
      .align-self-center.col-md-9
        .header-middle-info.float-right
          ul 
            li
              i.icon-number_1
              label Best Agric Investing
              p Service provider of 2021
