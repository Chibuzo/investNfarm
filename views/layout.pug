  .container
    .row.menu-item
      .col-lg-12
        nav#navigation1.navigation.header-nav.clearfix
          .nav-header
            a.mobile-logo(href='/index')
              img(src='/images/mobile_logo.png' alt='')
            .nav-toggle
          .nav-menus-wrapper.clearfix
            ul.nav-menu
              li.active
                a(href='/') Home
              li
                a(href='/about') About us
              li
                a(href='/projects') Our Projects
              li 
                a(href='/faq') FAQ
              li
                a(href='/contact') Contact Us
            .header-nav-right-info.align-to-right
              label
                i.icon.icon-phone3
                |  +44 345 678 903

block content 

footer.xs-footer-sec
  .container
    .footer-top-item
      .xs-back-to-top
        a.BackTo(href='#')
          i.fa.fa-angle-double-up
    .footer-item
      .row
        .col-lg-3
          .widgets
            .footer-logo
              a(href='/')
                img(src='/images/logo.png' alt='')
            p
              | Invest”N”Farm is an innovative digital agritech crowdfunding platform focused on arable and pastoral farming and other operations in Nigeria.
            ul.footer-social
              //- li
              //-   a(href='#')
              //-     i.fa.fa-facebook-f
              li
                a(href='https://twitter.com/investnfarm')
